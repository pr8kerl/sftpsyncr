; config file
[defaults]
port = 22
matchregexp = .*
;logfile = /tmp/sftpsyncr.log
lockdir = /tmp/sftpsyncr.lock
;insecureciphers = false
debug = true
;proxyserver = 192.168.0.100
;proxyport = 8080
;decrypt = false
; hex id of public key; private key of this public is used to decrypt
;decryptkeyid = ABCDEFG
;decryptpassphrase = passphrase
;encrypt = false
; hex id of public key used to encrypt files
;encryptkeyid = ABCDEFG
;encryptsuffix = ".gpg"
;publickeyring = "~/.gnupg/pubring.gpg"
;privatekeyring = "~/.gnupg/secring.gpg"

# config file
[profile "default"]
server = localhost
username = luser
password = letmein
;key = /home/luser/.ssh/id_rsa
;port = 22
;matchregexp = *
localdir = /tmp/local
remotedir = remote
;logfile = /tmp/sftpsyncr.log
;lockdir = /tmp/sftpsyncr.lock
; enable the use of cbc ciphers
insecureciphers = true
;debug = false
;proxyserver = 192.168.0.100
;proxyport = 8080
decrypt = true
; hex id of public key; private key of this public is used to decrypt
decryptkeyid = ABCDEFG
decryptpassphrase = passphrase
;encrypt = false
; hex id of public key used to encrypt files
;encryptkeyid = ABCDEFG
;encryptsuffix = ".gpg"
;publickeyring = "~/.gnupg/pubring.gpg"
;privatekeyring = "~/.gnupg/secring.gpg"

